<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.habib.teamfav">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/soccer_football"
        android:label="@string/app_name"
        android:roundIcon="@drawable/soccer_football"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".home.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- <activity android:name=".HomeActivity"></activity> -->
        <activity android:name=".detail.DetailActivity" />
        <activity android:name=".detail.TeamDetailActivity" />
        <activity android:name=".detail.PlayerActivity" />
        <activity android:name=".detail.PlayerDetailActivity"></activity>
    </application>

</manifest>